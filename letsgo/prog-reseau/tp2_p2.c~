#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
  int tab_entier[20];
  char* v;

  int n =0;
  
  while(n<20){
    printf("entrez un entier %d :",n);
    scanf("%s",v);

    int e;

    if(strcmp(v,"quit") == 0){
      printf("Quit\n");
    }

    if( (e = atoi(v)) != 0){
      tab_entier[n] = e;
      n++;
    }
  }

  return 0;
}

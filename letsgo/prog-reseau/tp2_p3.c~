#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int devine(char* mot){
  char c;

  int size = strlen(mot);
  
  int len[size];

  int szi =0;
  

  for(int i=0; i<size; i++){
    len[i] = 0;
  }

  while(1){
    c= getchar();

    if(c == '.'){
      return 0;
    }

    for(int i=0; i<size; i++){
      if( *(mot+i) == c){
        len[i] = 1;
	printf("\n%c ", c);
	szi++;
	break;
      }
    }

    //Verifier tout
    int find =1;
    for(int i=0; i<size; i++){
      if(len[i] == 0)
	find = 0;
    }

    if(find == 1)
      return szi;  
  }
}

int main(){
  printf("Jouer\n");
  if(devine("bonjour") == 0)
    printf("\ngagne!");
  else
    printf("\nPerdu!");
}

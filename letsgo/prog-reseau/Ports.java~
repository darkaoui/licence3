import java.io.*;
import java.net.*;

public class Ports{
    public static void main(String argv[]){

	if(argv.length = 4){
	    
	    int dep = Integer.parseInt(argv[1]);
	    int ari = Integer.parseInt(argv[2]);
	    String host = argv[3];

	    if(dep > ari){
		System.out.println("parametres non valide");
		return;
	    }

	    for(int i=dep; i<ari; i++){
		try{
		    Socket s = new Socket(host,i);
		    System.out.println("Ports ouverts "+i);
		    s.close();
		}catch{
		    System.out.println("---------------");
		}
	    }
	    
	}else
	    System.out.println("parametres non valide");
    }
}

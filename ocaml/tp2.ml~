let rec rev = function
    []   -> []
  | a::t -> a::rev

let rotation_d l=
  let rec parcours l1 lt= match l1,lt with
    |[],lt    -> failwith "liste vide"
    |a::[],lt -> [],a
    |a::t,lt  -> a::(parcours t lt)
  in
  let first= function a::_ -> a
  in
  let liste,lastEl = parcours l first l
  in
  liste
;;
  
